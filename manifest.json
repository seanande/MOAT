{
  "manifest_version": 2,

  "name": "MOAT",
  "description": "This extension will allow for quick authentication of online accounts",
  "author": "Sean Anderson",
  "version": "1.0",

  "browser_action": {
   "default_icon": "shield.png",
   "default_popup": "popup.html"
  },

  "permissions": [
   "activeTab",
   "tabs",
   "storage"
   ],
  "background": {
    "scripts": ["popup.js"]
  }
}
